(data.petfinder.pets.pet)) {\n          pets = data.petfinder.pets.pet;\n        } else {\n          pets = [data.petfinder.pets.pet];\n        }\n      } else {\n        pets = [];\n      }\n      this.setState({\n        pets\n      });\n    });\n  }\n\n  render() {\n    return (\n      <div>\n        <h1>Adopt Me</h1>\n        <div>\n          {this.state.pets.map(pet => {\n            let breed;\n\n            if (Array.isArray(pet.breeds.breed)) {\n              breed = pet.breeds.breed.join(\", \");\n            } else {\n              breed = pet.breeds.breed;\n            }\n            return (\n              <Pet\n                key={pet.id}\n                animal={pet.animal}\n                name={pet.name}\n                breed={breed}\n                media={pet.media}\n                location={`${pet.contact.city}, ${pet.contact.state}`}\n              />\n            );\n          })}\n        </div>\n      </div>\n    );\n  }\n}\n\nrender(<App />, document.getElementById(\"root\"));\n"},"lineCount":null}},"hash":"87fd644fb39dd20dd3708ea4c9beae68","cacheData":{"env":{"API_KEY":"6074af0e5d053573ad0427f70fccb5dc","API_SECRET":"3ce062e5eb152418a951d10d2f969fd0"}}}